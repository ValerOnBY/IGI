@model IEnumerable<Infastructure.Entities.Order>

<table class="table">
    <tr><th>Пользователь</th><th>Дата</th><th>Сумма</th><th>Колличество единиц</th></tr>

    @foreach (var order in Model)
    {
        <tr>
            <td>@order.User.Email</td>
            <td>@order.Date</td>
            <td>@order.Price</td>
            <td>@order.Count</td>
        </tr>
    }
</table>